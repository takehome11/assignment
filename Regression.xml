<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Regression Test Suite">
	<test name="Regression Test">
		<groups>
			<run>
				<include name="regression" />
			</run>
		</groups>
		<classes>
			<class name="com.test.infinite.TakeHomeTest">
			     <methods>
                    <include name="testSearchFirstItem"/>
                    <include name="testSearchAllItems"/>
                </methods>
			</class>
		</classes>
	</test>
</suite>
